<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @team.name %>
</p>

<p>
  <strong>Project:</strong>
  <%= @team.project.name %>
</p>

<p>
  <strong>Desenvolvedores:</strong>
  <table class="responsive nowrap table table-hover" id="dttb-projects" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="email">Email</th>
      <th></th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
    <% @team.developers.each do |developer|%>
      <tr>
        <td><%= developer.email %></td>
        <td><%= link_to 'Detalhes', developer %></td>
      </tr>
    <% end %>
  </thead>

</table>
</p>

<%= link_to 'Edit', edit_team_path(@team) %> |
<%= link_to 'Back', teams_path %>
